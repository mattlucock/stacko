{
	"name": "stacko",
	"version": "1.1.2",
	"type": "module",
	"scripts": {
		"check:format": "prettier --check . --loglevel warn",
		"check:lint": "eslint .",
		"check:ts": "tsc -p tsconfig.json --noEmit",
		"check": "pnpm run check:format && pnpm run check:lint && pnpm run check:ts",
		"build:main": "esbuild src/stack.ts --platform=neutral --bundle --outfile=dist/stack.js",
		"build:types": "tsc -p tsconfig.types.json",
		"build": "shx rm -rf dist && pnpm run build:main && pnpm run build:types",
		"test": "tsx --test test/**.ts"
	},
	"exports": {
		"types": "./dist/stack.d.ts",
		"import": "./dist/stack.js"
	},
	"devDependencies": {
		"@types/node": "^18.15.11",
		"esbuild": "^0.17.14",
		"eslint": "^8.37.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-config-standard-with-typescript": "^34.0.1",
		"prettier": "^2.8.7",
		"shx": "^0.3.4",
		"tsx": "^3.12.6",
		"typescript": "^5.0.2"
	},
	"files": [
		"dist"
	],
	"description": "Fast, tiny, type-safe stack data structure",
	"keywords": [
		"stack",
		"data structure",
		"push",
		"pop",
		"peek",
		"first in last out",
		"filo",
		"typescript",
		"ts",
		"tiny",
		"small",
		"lightweight",
		"fast"
	],
	"license": "Unlicense",
	"repository": {
		"type": "git",
		"url": "https://github.com/mattlucock/stacko.git"
	},
	"bugs": {
		"url": "https://github.com/mattlucock/stacko/issues"
	}
}
